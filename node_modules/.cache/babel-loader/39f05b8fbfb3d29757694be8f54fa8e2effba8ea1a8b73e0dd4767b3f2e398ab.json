e=()=>{return/*#__PURE__*/_jsxs(\"div\",{children:[/*#__PURE__*/_jsx(NavBar,{}),/*#__PURE__*/_jsx(LandingHeader,{}),/*#__PURE__*/_jsx(MainImage,{}),/*#__PURE__*/_jsx(NumberFigures,{}),/*#__PURE__*/_jsx(KeyPointsContainer,{}),/*#__PURE__*/_jsx(BeforeFooter,{}),/*#__PURE__*/_jsx(LandingFooter,{})]});};root.render(/*#__PURE__*/_jsx(LandingPage,{}));","map":{"version":3,"names":["React","createRoot","jsx","_jsx","jsxs","_jsxs","Fragment","_Fragment","importAll","r","images","keys","map","item","replace","require","context","root","document","getElementById","borderColor","border","helperText","hendelSubmit","e","preventDefault","inputval","console","log","value","HeplerTextStyle","color","NavBar","className","children","src","alt","href","LandingHeader","MainImage","NumberFigure","_ref","image","number","description","NumberFigures","IllustrationImages","_ref2","imageArea","customId","style","gridArea","concat","id","KeyPoints","_ref3","heading","text","textArea","KeyPointsContainer","BeforeFooter","LandingFooter","onSubmit","type","width","height","xmlns","fillRule","fill","d","viewBox","stroke","strokeWidth","strokeLinecap","strokeLinejoin","x","y","rx","ry","x1","x2","y1","y2","LandingPage","render"],"sources":["E:/zero to hero web development/Components/Completed/curved-section-page/src/index.js"],"sourcesContent":["import React from \"react\";\r\nimport { createRoot } from \"react-dom/client\";\r\nimport \"./style.css\";\r\n\r\nfunction importAll(r) {\r\n  let images = {};\r\n  r.keys().map((item) => {\r\n    images[item.replace(\"./\", \"\")] = r(item);\r\n  });\r\n  return images;\r\n}\r\n\r\nconst images = importAll(\r\n  require.context(\"./images\", false, /\\.(png|jpe?g|svg)$/)\r\n);\r\n\r\nconst root = createRoot(document.getElementById(\"root\"));\r\n\r\n// for incorrect email input\r\nconst borderColor='none'\r\nconst border='2px solid'\r\nconst helperText=''\r\n\r\n\r\nfunction hendelSubmit(e){\r\n  e.preventDefault()\r\n  const inputval= document.getElementById('email-input')\r\n  console.log(inputval.value)\r\n}\r\n\r\nconst HeplerTextStyle=()=>{\r\n  return({\r\n    color:'hsl(0, 100%, 63%)',\r\n  })\r\n}\r\n\r\nconst NavBar = () => {\r\n  return (\r\n    <nav className=\"wrapper\">\r\n      <img src={images[\"logo.svg\"]} alt=\"\" />\r\n      <a href=\"/\">Try it Free</a>\r\n    </nav>\r\n  );\r\n};\r\n\r\nconst LandingHeader = () => {\r\n  return (\r\n    <header className=\"wrapper\">\r\n      <h1>Build The Community Your Fans Will Love</h1>\r\n      <p className=\"wrapper-narrow\">\r\n        Huddle re-imagines the way we build communities. You have a voice, but\r\n        so does your audience. Create connections with your users as you engage\r\n        in genuine discussion.\r\n      </p>\r\n      <a href=\"/\">Get Started For Free</a>\r\n    </header>\r\n  );\r\n};\r\n\r\nconst MainImage = () => {\r\n  return (\r\n    <div className=\"main-image wrapper\">\r\n      <img src={images[\"screen-mockups.svg\"]} alt=\"\" />\r\n    </div>\r\n  );\r\n};\r\n\r\nconst NumberFigure = ({ image, number, description }) => {\r\n  return (\r\n    <div className=\"numberFigure\">\r\n      <img src={image} alt=\"\" />\r\n      <h1>{number}</h1>\r\n      <p>{description}</p>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst NumberFigures = () => {\r\n  return (\r\n    <div className=\"numberFigures wrapper\">\r\n      <NumberFigure\r\n        image={images[\"icon-communities.svg\"]}\r\n        number={\"1.4k+\"}\r\n        description={\"Comunities Formed\"}\r\n      />\r\n      <NumberFigure\r\n        image={images[\"icon-messages.svg\"]}\r\n        number={\"2.7m+\"}\r\n        description={\"Messages Sent\"}\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nconst IllustrationImages = ({ image, imageArea, customId }) => {\r\n  return (\r\n    <div style={{ gridArea: `${imageArea}` }}>\r\n      <div className=\"illustration-img\">\r\n        <img src={image} alt=\"\" id={customId} />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst KeyPoints = ({ heading, text, textArea, customId }) => {\r\n  return (\r\n    <div className=\"wrapper\" id={customId} style={{ gridArea: `${textArea}` }}>\r\n      <div className=\"illustration-text\">\r\n        <h2>{heading}</h2>\r\n        <p className=\"text-align\">{text}</p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst KeyPointsContainer = () => {\r\n  return (\r\n    <article>\r\n      <img\r\n        src={images[\"bg-section-top-desktop-1.svg\"]}\r\n        className=\" desk-top\"\r\n        alt=\"\"\r\n      />\r\n      <img\r\n        src={images[\"bg-section-top-mobile-1.svg\"]}\r\n        className=\" mob-top\"\r\n        alt=\"\"\r\n      />\r\n      <article className=\"article1\">\r\n        <div className=\"desk-wrapper wrapper\">\r\n          <IllustrationImages\r\n            image={images[\"illustration-grow-together.svg\"]}\r\n            imageArea={\"img1\"}\r\n            customId={\"first-key-img\"}\r\n          />\r\n          <KeyPoints\r\n            heading={\"Grow Together\"}\r\n            text={\r\n              \"Generate meaningful discussions with your audience and build a strong, loyal community. Think of the insightful conversations you miss out on with a feedback form. \"\r\n            }\r\n            textArea={\"text1\"}\r\n            customId={\"first-key-text\"}\r\n          />\r\n        </div>\r\n      </article>\r\n      <img\r\n        src={images[\"bg-section-bottom-mobile-1.svg\"]}\r\n        className=\" mob-bottom\"\r\n        alt=\"\"\r\n      />\r\n      <img\r\n        src={images[\"bg-section-bottom-desktop-1.svg\"]}\r\n        className=\" desk-bottom\"\r\n        alt=\"\"\r\n      />\r\n      <article className=\"article2\">\r\n        <div className=\"desk-wrapper wrapper flex-reverse\">\r\n          <IllustrationImages\r\n            image={images[\"illustration-flowing-conversation.svg\"]}\r\n            imageArea={\"img2\"}\r\n            customId={\"second-key-img\"}\r\n          />\r\n          <KeyPoints\r\n            heading={\"Flowing Conversations\"}\r\n            text={\r\n              \"You worldn't paginate a conversation in real life, so why do it online? Our threads have just-in-time loading for a more natual flow.\"\r\n            }\r\n            textArea={\"text2\"}\r\n            customId={\"second-key-text\"}\r\n          />\r\n        </div>\r\n      </article>\r\n      <img\r\n        src={images[\"bg-section-top-desktop-2.svg\"]}\r\n        className=\" desk-top\"\r\n        alt=\"\"\r\n      />\r\n      <img\r\n        src={images[\"bg-section-top-mobile-2.svg\"]}\r\n        className=\" mob-top\"\r\n        alt=\"\"\r\n      />\r\n      <article className=\"article3\">\r\n        <div className=\"desk-wrapper wrapper\">\r\n          <IllustrationImages\r\n            image={images[\"illustration-your-users.svg\"]}\r\n            imageArea={\"img3\"}\r\n            customId={\"third-key-img\"}\r\n          />\r\n          <KeyPoints\r\n            heading={\"Your Users\"}\r\n            text={\r\n              \"It takes no time at all to integrate Huddle with your app's authentication solution. This means, once signed in to your app, your users can start chatting immediately.\"\r\n            }\r\n            textArea={\"text3\"}\r\n            customId={\"third-key-text\"}\r\n          />\r\n        </div>\r\n      </article>\r\n      <img\r\n        src={images[\"bg-section-bottom-mobile-2.svg\"]}\r\n        className=\" mob-bottom\"\r\n        alt=\"\"\r\n      />\r\n      <img\r\n        src={images[\"bg-section-bottom-desktop-2.svg\"]}\r\n        className=\" desk-bottom\"\r\n        alt=\"\"\r\n      />\r\n    </article>\r\n  );\r\n};\r\n\r\nconst BeforeFooter = () => {\r\n  return (\r\n    <div className=\" wrapper before-footer\">\r\n      <h2>Ready To Build Your Community?</h2>\r\n      <a href=\"/\">Get Started For Free</a>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst LandingFooter = () => {\r\n  return (\r\n    <>\r\n 